pipeline { 
    agent any

    stages{

        stage('Dev'){
            steps{
            echo "DEV"
        }
        
        }

        stage("Test"){
             steps{
                catchError(buildResult: "SUCCESS", stageResult: "FAILURE"){
            sh'''
            sleep 5
            echo "Test"
            exit 1
            '''
                }
        }
        }

        stage("Build"){
            steps{
            echo "BUILD"
        }
        }
    
    
    }


}